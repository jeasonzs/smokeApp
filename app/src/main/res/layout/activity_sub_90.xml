<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" 
    android:background="@drawable/background">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="40dp"
        android:src="@drawable/logo" />
    
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="20dp"
        android:src="@drawable/nosmoke_text" />
    

    
    <LinearLayout 
        android:id="@+id/linearLayoutSub90DataContent"
        android:layout_width="336.5dp"
        android:layout_height="320.5dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:orientation="vertical"
        android:paddingLeft="35.5dp"
	    android:paddingTop="34.5dp"
	    android:paddingRight="35dp"
	    android:paddingBottom="34.5dp"
        android:background="@drawable/sub_90_data_frame">

        <FrameLayout
            android:id="@+id/frameLayoutSubData1_30"
            android:layout_width="match_parent"
            android:layout_height="match_parent" 
            android:layout_weight="1"
            android:paddingLeft="3dp"
		    android:paddingTop="3dp"
		    android:paddingRight="3dp"
		    android:paddingBottom="13.5dp">
        </FrameLayout>
        
        <FrameLayout
            android:id="@+id/frameLayoutSubData31_60"
            android:layout_width="match_parent"
            android:layout_height="match_parent" 
            android:layout_weight="1"
            android:paddingLeft="3dp"
		    android:paddingTop="8dp"
		    android:paddingRight="3dp"
		    android:paddingBottom="8dp">
        </FrameLayout>
        
        <FrameLayout
            android:id="@+id/frameLayoutSubData61_90"
            android:layout_width="match_parent"
            android:layout_height="match_parent" 
            android:layout_weight="1"
            android:paddingLeft="3dp"
		    android:paddingTop="13.5dp"
		    android:paddingRight="3dp"
		    android:paddingBottom="3dp">
        </FrameLayout>
        
    </LinearLayout>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@id/linearLayoutSub90DataContent"
        android:layout_centerHorizontal="true" 
        android:src="@drawable/sub_90" />
        
		

    <ImageButton
        android:id="@+id/imageButtonSub90Return"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/linearLayoutSub90DataContent"
        android:background="@drawable/sub_return_icon" />

    

</RelativeLayout>