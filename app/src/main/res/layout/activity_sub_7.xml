<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background" >

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="40dp"
        android:src="@drawable/logo" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="50dp"
        android:src="@drawable/nosmoke_text" />
    
	<FrameLayout
	    android:id="@+id/frameLayoutSub7DataContent"
	    android:layout_width="335dp"
	    android:layout_height="303.5dp"
	    android:layout_centerHorizontal="true"
	    android:layout_centerVertical="true"
	    android:paddingLeft="48.5dp"
	    android:paddingTop="46.5dp"
	    android:paddingRight="48.5dp"
	    android:paddingBottom="99dp"
	    android:background="@drawable/sub_7_data_frame" >

    </FrameLayout>
    

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@id/frameLayoutSub7DataContent"
        android:layout_centerHorizontal="true"
        android:src="@drawable/sub_7" />

    <ImageButton
        android:id="@+id/imageButtonSub7Return"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/frameLayoutSub7DataContent"
        android:layout_centerHorizontal="true"
        android:background="@drawable/sub_return_icon" />

    

</RelativeLayout>